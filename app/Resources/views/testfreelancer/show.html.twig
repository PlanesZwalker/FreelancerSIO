{% extends 'base.html.twig' %}

{% block body %}
    <h1>Testfreelancer</h1>

    <table>
        <tbody>
            <tr>
                <th>Resultat</th>
                <td>{{ testfreelancer.resultat }}</td>
            </tr>
            <tr>
                <th>Datepassage</th>
                <td>{% if testfreelancer.datePassage %}{{ testfreelancer.datePassage|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('testfreelancer_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('testfreelancer_edit', { 'id': testfreelancer.freelancer }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
