{% extends 'base.html.twig' %}
{% block titre %}

        <h1 id="logo"><a class="bigtitle">Sociétés</a></h1>  

{% endblock %}

{% block body %}    
    <div class="formulairetop">
        <h2 class="mysubtitle">Ajouter une Société</h2>
        <div class="mycontentform">
            <ul>
                <li class="returnlist">
                    <a data-ajax="false" class="btn btn-default" href="{{ path('societe_index') }}">Retourner à la liste</a>
                </li>
            </ul>
            <table  class="tableautitle">
                <tbody>
                
                    {{ form_start(form) }}

                        {{ form_row(form.denomination) }}
                        {{ form_row(form.presentation) }}                        
                        {{ form_row(form.adresse) }}
                        {{ form_row(form.siret) }}
                        {{ form_row(form.tel) }}
                        {{ form_row(form.logo) }}
                        {{ form_row(form.user) }}
                        
                        <input type="submit" value="Enregistrer le profil" />
                    {{ form_end(form) }}
                </tbody>
             </table> 
            <ul>
           
            </ul>
        </div>
    </div>
{% endblock %}
