    <img src="bureau.jpg" style="width:6vw; float:left; margin-left:4vw;margin-top:7vw" alt="" />
    <img src="loupe.svg" style="width:8vw; float:right; margin-right:15vw;margin-top:5vw" alt="" />
{% endblock %}

{% block body %}


    <div id="page-wrapper">
