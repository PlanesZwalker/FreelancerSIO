    app.request.cookies
    app.request.headers
    app.request.content
    app.request.languages
    app.request.charsets
    app.request.acceptableContentTypes
    app.request.pathInfo
    app.request.requestUri
    app.request.baseUrl
    app.request.basePath
    app.request.method
    app.request.format
    
app.security
    app.security.token
    app.security.key
    app.security.user
    app.security.roles
    app.security.authenticated
    app.security.attributes
    app.security.alwaysAuthenticate
    app.session
    app.session.locale
    app.session.defaultLocale
    app.session.saved

    #}



    </div>
</div>
{% endblock %}


