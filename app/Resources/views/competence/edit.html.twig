{% extends 'base.html.twig' %}
{% block titre %}

        <h1 id="logo"><a class="bigtitle">Compétences</a></h1>  

{% endblock %}

{% block body %}    
    <div class="formulairetop">
        <h2 class="mysubtitle">Edition d'une Compétence</h2>
        <div class="mycontentform">
            <ul>
                <li class="returnlist">
                    <a class="btn btn-default" href="{{ path('competence_index') }}">Retourner à la liste</a>
                </li>
            </ul>
            <table  class="tableautitle">
                <tbody>
            {{ form_start(edit_form) }}
                <tr>
                    <th class="mytdtabshow"></th>
                </tr>        
                <tr>
                    <th class="mytdtabshow2"></th>
                </tr>        
                <tr>
                    <th class="mytdtabshow" style="display:none;">{{ form_widget(edit_form.idContrat) }}</th>
                </tr>
                </tbody>
            </table>   
            <input class="myeditinput" type="submit" value="Enregistrer les modifications" />
            {{ form_end(edit_form) }}
            {{ dump(edit_form) }}
            <ul>
                <li class="mydeleteli ">
                    {{ form_start(delete_form) }}
                        <input type="submit" value="Delete">
                    {{ form_end(delete_form) }}
                </li>
            </ul>
        </div>
    </div>
{% endblock %}
